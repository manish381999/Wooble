<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_discover"
    >


    <fragment
        android:id="@+id/navigation_discover"
        android:name="com.wooble.wooble.ui.Discover.DiscoverFragment"
        android:label="Discover"
        tools:layout="@layout/fragment_discover">
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/navigation_search" />
    </fragment>


    <fragment
        android:id="@+id/navigation_search"
        android:name="com.wooble.wooble.ui.Search.SearchFragment"
        android:label="Search"
        tools:layout="@layout/fragment_search">
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/navigation_notification" />
    </fragment>

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.wooble.wooble.ui.Notification.NotificationFragment"
        android:label="Notification"
        tools:layout="@layout/fragment_notification">

        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/navigation_profile" />
    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.wooble.wooble.ui.Profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile">

    </fragment>



</navigation>